{
  "name": "shopify-prime-dc",
  "version": "2.5.70",
  "description": "Shopify Prime is a NodeJS library built to help developers easily authenticate and make calls against the Shopify API. It was inspired by and borrows heavily from ShopifySharp.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build": "tsc -p . && npm run declarations",
    "declarations": "node ./.tasks/add-custom-declarations",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nozzlegear/shopify-prime.git"
  },
  "keywords": [
    "shopify",
    "gearworks",
    "ecommerce",
    "api"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nozzlegear/shopify-prime/issues"
  },
  "homepage": "https://github.com/nozzlegear/shopify-prime#readme",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "crypto-js": "^4.2.0",
    "jsuri": "^1.3.1",
    "node-fetch": "^2.7.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jsuri": "^1.3.34",
    "@types/node": "^18.19.26",
    "@types/node-fetch": "^2.6.11",
    "glob": "^7.2.3",
    "mkdirp": "^0.5.6",
    "typescript": "^5.4.3"
  }
}
